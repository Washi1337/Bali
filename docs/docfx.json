{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/Bali/Bali.csproj",
            "src/Bali.IO/Bali.IO.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
            "api/**"
        ]
      },
      {
        "files": [
            "*.md",
            "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
        "_appTitle": "Bali"
    }
  }
}
